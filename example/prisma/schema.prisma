// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:sqlite.db"
}

model User {
  id        Int      @id
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}
